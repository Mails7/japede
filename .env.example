# Supabase Configuration
# Copie este arquivo para .env.local e preencha com suas credenciais reais do Supabase

# URL do seu projeto Supabase (formato correto)
NEXT_PUBLIC_SUPABASE_URL=https://seu-projeto-id.supabase.co

# Chave anônima do Supabase (encontre no painel do Supabase > Settings > API)
NEXT_PUBLIC_SUPABASE_ANON_KEY=sua-chave-anonima-aqui

# Chave de serviço do Supabase (opcional, para operações administrativas)
SUPABASE_SERVICE_ROLE_KEY=sua-chave-servico-aqui

# Production Environment
NODE_ENV=development